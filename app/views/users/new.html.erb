<%= form_tag '/users' do %>
  <%= text_field_tag 'user[name]' %>
  <%= text_field_tag 'user[height]' %>
  <%= text_field_tag 'user[happiness]' %>
  <%= text_field_tag 'user[nausea]' %>
  <%= text_field_tag 'user[tickets]' %>
  <%= password_field_tag 'user[password]' %>
  <%= check_box_tag 'user[admin]'%>
  <%= submit_tag 'Create User' %>
<% end %>
